lli N = 1000005;
vector<lli>fact(N+1);
void fact_mod()
{
    fact[0] = 1;
    fact[1] = 1;
    for(lli i = 2; i<=N; i++)
    {
        fact[i] = (fact[i-1]*i)%M;
    }
}
