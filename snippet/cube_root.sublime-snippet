	<content><![CDATA[
vector<lli>cube_root;
//store: 1 8 27 64 125-----<=n
void cube_root_number(lli n)
{
    for(lli i = 1; i<=1000000; i++)
    {
        if((i*i*i)>n)return;
        cube_root.push_back(i*i*i);
    }
}
]]></content>
